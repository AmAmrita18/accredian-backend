generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("MYSQL_DATABASE_URL")
}

model FormDetails {
  id                            Int      @id @default(autoincrement())
  fullName                      String
  email                         String   
  message                       String   
  createdAt                     DateTime @default(now())
  updatedAt                     DateTime @default(now())
}