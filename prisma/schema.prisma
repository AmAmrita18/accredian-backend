generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a directÂ connection

}

model FormDetails {
  id                            Int      @id @default(autoincrement())
  fullName                      String
  email                         String   
  message                       String   
  createdAt                     DateTime @default(now())
  updatedAt                     DateTime @default(now())
}